{
    "name": "laravel-zero/framework",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "console", "zero", "cli"],
    "homepage": "http://laravel-zero.com",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/foundation": "5.6.x-dev",
        "illuminate/config": "5.6.*-dev",
        "illuminate/console": "5.6.*-dev",
        "illuminate/container": "5.6.*-dev",
        "illuminate/events": "5.6.*-dev",
        "illuminate/cache": "5.6.*-dev",
        "illuminate/filesystem": "5.6.*-dev",
        "nunomaduro/collision": "1.1.*",
        "nunomaduro/laravel-desktop-notifier": "1.3.*",
        "mtdowling/cron-expression": "^1.2",
        "league/flysystem": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "squizlabs/php_codesniffer": "^2.3",
        "illuminate/database": "5.6.*-dev",
        "illuminate/log": "5.6.*-dev"
    },
    "autoload": {
        "psr-4": {
            "LaravelZero\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\": "tests/Application/app"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    }
}
